def isValid(s):
  stack = []
  mapping = { ')': '(', '}': '{', ']': '[' }
  for char in s:
    if char in mapping:
      if not stack or stack.pop() != mapping [char]:
        return False
    else:
      stack.append(char)
    
  return len(stack) == 0

string = input()
result = isValid(string)
if result:
  print("true")
else:
  print("false")  
  
  ##Sample Input1: (())
  ##Sample Output1: True
  
  ##Sample Input1: (()}
  ##Sample Output1: False
